		STEP propagate_effect_required
		IF ((NOT required OF Loss_of_GRID) AND (NOT required OF
		  LossOfLine_1)) AND (NOT required OF LossOfLine2)
		THEN required OF GRID <-- FALSE;
	  xx4
		GROUP V3_simu_group
