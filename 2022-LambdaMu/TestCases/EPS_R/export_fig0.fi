	CONSTANT
	  calculate_required
		DOMAIN 'fn_fathers_and_trig' 'fn_fathers_opp_trig' 'always_true' 'always_false' 'equal_to_S_trig' 'opposite_to_S_trig'
		= 'fn_fathers_and_trig';
	  force_relevant_events
		DOMAIN BOOLEAN
		= FALSE;
	ATTRIBUTE
	  S
		LABEL "%OBJECT"
		DOMAIN BOOLEAN
		REINITIALISATION FALSE;
	  relevant_evt
		DOMAIN BOOLEAN
		REINITIALISATION FALSE;
	CONSTANT
	  mu
		DOMAIN REAL
		= 0.0005;
	  when_to_check
		DOMAIN 'not_req_to_req' 'req_to_not_req'
		= 'not_req_to_req';
	  gamma
		DOMAIN REAL
		= 0.2;
	  failI_FROZEN
		DOMAIN BOOLEAN
		= FALSE;
	(*FAILURE*)
	ATTRIBUTE
	  failI
		LABEL "instantaneous failure %OBJECT"
		DOMAIN BOOLEAN
		= FALSE;
	ATTRIBUTE
	  to_be_fired
		DOMAIN BOOLEAN
		REINITIALISATION FALSE;
	  already_standby
		DOMAIN BOOLEAN
		= FALSE;
	  already_required
		DOMAIN BOOLEAN
		= FALSE;
	INTERACTION
	  xx3
		GROUP V3_simu_group
		STEP propagate_effect_required
		IF NOT required OF loss_of_house_operation
		THEN required OF on_demand_house <-- FALSE;
	  xx4
		GROUP V3_simu_group
		STEP propagate_effect_required
		IF relevant_evt OF loss_of_house_operation AND (NOT S OF
		  loss_of_house_operation)
		THEN relevant_evt OF on_demand_house <-- TRUE;
	  xx5
		GROUP V3_simu_group
		STEP propagate_leaves
		THEN already_S OF on_demand_house <-- S OF on_demand_house;
	  xx6
		GROUP V3_simu_group
		STEP initialization
		IF failI OF on_demand_house = TRUE
		THEN S OF on_demand_house <-- TRUE;
	  xx20
		GROUP V3_simu_group
		STEP propagate_leaves
		IF (NOT required OF on_demand_house) AND ((NOT already_standby
		  OF on_demand_house) AND (NOT already_required OF
		  on_demand_house))
		THEN already_standby OF on_demand_house <-- TRUE;
	  xx22
		GROUP V3_simu_group
		STEP tops
		IF required OF on_demand_house AND already_standby OF
		  on_demand_house
		THEN to_be_fired OF on_demand_house <-- TRUE;
	OCCURRENCE
	  xx23
		GROUP V3_simu_group
		IF (failI OF on_demand_house = FALSE) AND (to_be_fired OF
		  on_demand_house AND relevant_evt OF on_demand_house)
		MAY_OCCUR
		  FAULT failI
		  LABEL "instantaneous failure %OBJECT"
		  DIST INS (0.2)
		  INDUCING failI OF on_demand_house <-- TRUE,
			already_standby OF on_demand_house <-- FALSE,
			already_required OF on_demand_house <-- FALSE
		OR_ELSE
		  TRANSITION good
		  DIST INS (0.8)
		  INDUCING already_standby OF on_demand_house <-- FALSE,
			already_required OF on_demand_house <-- FALSE;
	  xx24
		GROUP V3_simu_group
		IF failI OF on_demand_house = TRUE
		MAY_OCCUR
		  REPAIR rep
		  DIST EXP (0.0005)
		  INDUCING failI OF on_demand_house <-- FALSE;

