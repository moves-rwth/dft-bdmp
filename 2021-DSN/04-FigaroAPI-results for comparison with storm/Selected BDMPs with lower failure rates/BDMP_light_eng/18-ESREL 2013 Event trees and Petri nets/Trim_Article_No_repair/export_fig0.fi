		STEP propagate_effect_required
		IF NOT required OF A_ND_by_ship
		THEN required OF radar <-- FALSE;
	  xx4
		GROUP V3_simu_group
		STEP propagate_effect_required
		IF relevant_evt OF A_ND_by_ship AND (NOT S OF A_ND_by_ship)
		THEN relevant_evt OF radar <-- TRUE;
	  xx5
