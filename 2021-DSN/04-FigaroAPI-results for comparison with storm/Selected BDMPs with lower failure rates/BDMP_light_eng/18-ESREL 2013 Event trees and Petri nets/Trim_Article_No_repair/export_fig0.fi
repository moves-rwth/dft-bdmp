	  xx22
		GROUP V3_simu_group
		STEP tops
		IF required OF radar AND already_standby OF radar
		THEN to_be_fired OF radar <-- TRUE;
	OCCURRENCE
	  xx23
		GROUP V3_simu_group
		IF (failI OF radar = FALSE) AND (to_be_fired OF radar AND
